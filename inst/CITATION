year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{nomisr}: Access nomis UK labour market data on with R",
         author = c(person("Evan", "Odell")),
         year = year,
         note = note,
         doi = "",
         url = c("https://docs.evanodell.com/nomisr" ))


